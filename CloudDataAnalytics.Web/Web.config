<configuration>
  <configSections>
    <section name="ServiceProviderConfiguration" type="CloudDataAnalytics.Web.Components.ServiceProviderConfiguration, CloudDataAnalytics.Web, Version=1.0.0.0, Culture=neutral" />
  </configSections>
  <appSettings>
    <add key="webpages:Version" value="2.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="PreserveLoginUrl" value="true" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="CertificatePassword" value="123123123" />
    <add key="PrivateCertificate" value="~/MyCertificates/servmancertkey.sp.p12" />
